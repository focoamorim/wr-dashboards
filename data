<script>
Chart.defaults.font.family = "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif";
Chart.defaults.color = '#495057';
Chart.defaults.plugins.tooltip.backgroundColor = 'rgba(26, 26, 46, 0.9)';
Chart.defaults.plugins.tooltip.padding = 12;
Chart.defaults.plugins.tooltip.cornerRadius = 8;
Chart.defaults.plugins.tooltip.titleFont = { size: 14, weight: '600' };
Chart.defaults.plugins.tooltip.bodyFont = { size: 13 };

const clientSideChartColors = ["rgba(255, 107, 53, 0.8)","rgba(247, 147, 30, 0.8)","rgba(252, 176, 69, 0.8)","rgba(102, 126, 234, 0.8)","rgba(118, 75, 162, 0.8)","rgba(240, 147, 251, 0.8)","rgba(245, 87, 108, 0.8)","rgba(17, 153, 142, 0.8)","rgba(56, 239, 125, 0.8)","rgba(79, 172, 254, 0.8)","rgba(0, 242, 254, 0.8)","rgba(255, 193, 7, 0.8)"];

function animateValue(element, start, end, duration, numPrefix, numDecimals) {
  const range = end - start;
  if (range === 0 && start === 0 && element.textContent.includes(localFormatNumber(0, numDecimals, numPrefix))) { // Check if already zero
      // Do nothing if already zero and supposed to be zero, to avoid re-animation from 0 to 0
  } else if (range === 0) {
       element.textContent = localFormatNumber(end, numDecimals, numPrefix);
       return;
  }

  const increment = range / (duration / 16);
  let current = start;
  const timer = setInterval(() => {
    current += increment;
    if ((increment > 0 && current >= end) || (increment < 0 && current <= end)) {
      current = end;
      clearInterval(timer);
    }
    element.textContent = localFormatNumber(current, numDecimals, numPrefix);
  }, 16);
}

function localFormatNumber(value, decimals = 2, prefix = 'U$ ') {
  if (typeof value !== 'number' || isNaN(value)) return prefix + Number(0).toFixed(decimals);
  return prefix + Number(value).toLocaleString('en-US', { minimumFractionDigits: decimals, maximumFractionDigits: decimals });
}

document.addEventListener('DOMContentLoaded', () => {
  document.querySelectorAll('.metric-value').forEach(element => {
    const value = parseFloat(element.getAttribute('data-value'));
    if (!isNaN(value)) {
      if (element.textContent !== localFormatNumber(value, 2, 'U$ ')) {
        animateValue(element, 0, value, 1000, 'U$ ', 2);
      } else {
         element.textContent = localFormatNumber(value, 2, 'U$ '); 
      }
    }
  });

  document.querySelectorAll('.bar-fill, .progress-fill, .timeline-fill').forEach(element => {
    const width = element.style.width;
    element.style.width = '0%';
    setTimeout(() => {
      element.style.transition = 'width 0.5s ease-out';
      element.style.width = width;
    }, 100);
  });

  
    const costByPurchaseCtx = document.getElementById('ec2CostByPurchaseOption');
    if (costByPurchaseCtx) {
      new Chart(costByPurchaseCtx, {
        type: 'doughnut',
        data: {
          labels: ["Spot","On Demand"],
          datasets: [{
            data: [51.870000000000005,36.190000000000005],
            backgroundColor: ["#f56c1f","#f7a822"],
            borderWidth: 2,
            borderColor: '#ffffff'
          }]
        },
        options: {
          responsive: true, maintainAspectRatio: false,
          plugins: { legend: { position: 'bottom', labels: { padding: 15, font: { size: 12 }}},
            tooltip: { callbacks: { label: (context) => {
                  const label = context.label || '';
                  const value = localFormatNumber(context.parsed);
                  const total = context.dataset.data.reduce((a, b) => a + b, 0);
                  const percentage = total > 0 ? ((context.parsed / total) * 100).toFixed(1) : 0;
                  return [`${label}: ${value}`, `${percentage}% do total`];
            }}}}
        }
      });
    }

    const hoursByPurchaseCtx = document.getElementById('ec2HoursByPurchaseOption');
    if (hoursByPurchaseCtx) {
      new Chart(hoursByPurchaseCtx, {
        type: 'doughnut',
        data: {
          labels: ["Spot","On Demand"],
          datasets: [{
            data: [1095.77,312.02],
            backgroundColor: ["#f56c1f","#f7a822"],
            borderWidth: 2,
            borderColor: '#ffffff'
          }]
        },
        options: {
          responsive: true, maintainAspectRatio: false,
          plugins: { legend: { position: 'bottom', labels: { padding: 15, font: { size: 12 }}},
            tooltip: { callbacks: { label: (context) => {
                  const label = context.label || '';
                  const value = (context.parsed || 0).toFixed(2);
                  const total = context.dataset.data.reduce((a, b) => a + b, 0);
                  const percentage = total > 0 ? ((context.parsed / total) * 100).toFixed(1) : 0;
                  return [`${label}: ${value} horas`, `${percentage}% do total`];
            }}}}
        }
      });
    }

    const costByTypeCtx = document.getElementById('ec2CostByInstanceType');
    if (costByTypeCtx) {
      new Chart(costByTypeCtx, {
        type: 'bar',
        data: {
          labels: ["T3.LARGE","M5A.8XLARGE","T3A.2XLARGE","T3.XLARGE","T2.MEDIUM","M5.LARGE","M5.4XLARGE","M5A.LARGE","M5A.XLARGE"],
          datasets: [{
            label: 'Custo Total',
            data: [45,20.160000000000004,15.01,6.56,0.48,0.33999999999999997,0.31,0.11,0.09000000000000001],
            backgroundColor: 'rgba(255, 107, 53, 0.8)',
            borderColor: 'rgba(255, 107, 53, 1)',
            borderWidth: 1
          }]
        },
        options: {
          responsive: true, maintainAspectRatio: false, indexAxis: 'y',
          scales: { x: { beginAtZero: true, ticks: { callback: (value) => localFormatNumber(value, 2, 'U$ ') }}, y: { ticks: { font: { size: 10 }}} },
          plugins: { legend: { display: false }, tooltip: { callbacks: { label: (context) => localFormatNumber(context.parsed.x, 2, 'U$ ') }}}
        }
      });
    }

    const hoursByTypeCtx = document.getElementById('ec2HoursByInstanceType');
    if (hoursByTypeCtx) {
      new Chart(hoursByTypeCtx, {
        type: 'bar',
        data: {
          labels: ["T3.LARGE","T3A.2XLARGE","T2.MEDIUM","T3.XLARGE","M5A.8XLARGE","M5.LARGE","M5.4XLARGE","M5A.LARGE","M5A.XLARGE"],
          datasets: [{
            label: 'Horas Totais',
            data: [1002.78,179.64,120,92.07000000000001,9.120000000000001,2.18,0.92,0.77,0.31],
            backgroundColor: 'rgba(102, 126, 234, 0.8)',
            borderColor: 'rgba(102, 126, 234, 1)',
            borderWidth: 1
          }]
        },
        options: {
          responsive: true, maintainAspectRatio: false, indexAxis: 'y',
          scales: { x: { beginAtZero: true, ticks: { callback: (value) => (value || 0).toFixed(0) + ' hrs' }}, y: { ticks: { font: { size: 10 }}} },
          plugins: { legend: { display: false }, tooltip: { callbacks: { label: (context) => (context.parsed.x || 0).toFixed(2) + ' horas' }}}
        }
      });
    }
  
});
</script>
